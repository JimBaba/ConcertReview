<% layout('/layouts/boilerplate') %>
<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger" style="word-wrap: break-word;" role="alert">
            <h4 class="alert-heading">
                <%= err.msg %>
            </h4>
            <p><%= err.stack %></p>
            <hr>
            <p class="mb-0">StatusCode: <%= err.statusCode %></p>
        </div>
    </div>
</div>
